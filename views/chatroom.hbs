<div class="chat">
  <header class="chatroom-name"></header>
  <div class="chat-room-info">
    <div class="chat-users"></div>
    <div class="chat-box"></div>
  </div>
  <form class="chat-submit">
    <input type="text" id="msg" maxlength="600" placeholder="write here...." autocomplete="off" required>
    <button class="submit-btn"><i class="far fa-paper-plane"></i>Send</button>
  </form>
</div>
